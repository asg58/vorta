# VORTA ULTRA API Configuration
# Copy to .env and update with your actual values

# Database Configuration
DATABASE_URL=postgresql://your_user:your_password@localhost:5432/vorta_db

# Redis Configuration  
REDIS_URL=redis://localhost:6379/0

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=True

# Security
SECRET_KEY=your-super-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Logging
LOG_LEVEL=INFO
